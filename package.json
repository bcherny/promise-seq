{
  "name": "promise-seq",
  "version": "2.0.0",
  "description": "Lazy-execute promises in sequence",
  "main": "index.js",
  "main:esnext": "./index.es2015.js",
  "typings": "./index.d.ts",
  "scripts": {
    "build": "tsc -d -m es2015 --outDir .; mv ./index.js ./index.es2015.js; tsc -m commonjs --outDir .;",
    "watch": "tsc -w",
    "test": "ava",
    "postinstall": "typings install"
  },
  "keywords": [
    "lazy",
    "promise",
    "sequential",
    "sequence",
    "sync",
    "synchronous",
    "synchronize"
  ],
  "author": "Boris Cherny <boris@performancejs.com> (http://performancejs.com/)",
  "license": "CC0",
  "devDependencies": {
    "ava": "^0.15.2",
    "nodeunit": "^0.9.1",
    "typescript": "^1.8.10"
  },
  "engine": "node >= 0.12",
  "dependencies": {
    "typings": "^1.0.4"
  }
}
